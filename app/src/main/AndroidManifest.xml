<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetointegrador">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_sr8"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_sr8"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AnuncioActivity"></activity>
        <activity android:name=".FotoActivity" />
        <activity android:name=".FeedActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroActivity" />
        <activity android:name=".CadastrolocalActivity" />
        <activity android:name=".LocalActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>